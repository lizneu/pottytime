extends layout

block content
  .container
    .view#findPottyView
      .findView.row#mapView
        #mapCanvas(style="height: 400px")
      .findView#listView
      .row#toggleView
        .col-xs-6.toggleBtn.btn.btn-primary#mapBtn Map
        .col-xs-6.toggleBtn.btn.btn-primary#listBtn List
    .view#detailView
    .view#addNewView
  script(src="//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js")
  script(type="text/javascript" src="js/bootstrap.min.js")
  script(type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDsPkG9N-Ip2foqVhbZYivMeZlCPHI4eEE&sensor=true")
  script(type="text/javascript" src="js/ourapp.js")